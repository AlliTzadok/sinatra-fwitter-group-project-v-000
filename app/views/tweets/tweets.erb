<!-- <h4>Welcome, <%= current_user.username %></h4> -->
<br>
<h2>@<%=current_user.username%> Tweets</h2>

<% @tweets.each do |tweet| %>
  <p class="timestamp"><%= tweet.created_at %>:</p>
  <p class="tweets"><%= tweet.content %></p>
  <p><%=tweet.user.username%></p>
  <br><br>
<%end%>


<form action="/tweets/new" method="get">
  <input type="submit" value="Create new Tweet?">

</form>
